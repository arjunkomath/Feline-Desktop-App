{
  "name": "feline",
  "version": "0.0.1",
  "description": "Feline Mac App",
  "main": "main.js",
  "scripts": {
    "start": "ENVIRONMENT=DEV ./node_modules/.bin/electron .",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "osx-package": "./node_modules/.bin/webpack -p && ./node_modules/electron-packager/cli.js ./ Feline --out ./bin --platform=darwin --arch=x64 --version=0.35.0 --overwrite --ignore=\"ignore|bin|node_modules\" --icon feline.icns --asar=True",
    "win-package": "./node_modules/.bin/webpack -p && ./node_modules/electron-packager/cli.js ./ Feline --out ./bin --platform=win32 --arch=x64 --version=0.35.0 --overwrite --ignore=\"ignore|bin|node_modules\""
  },
  "keywords": [
    "product",
    "hunt",
    "mac",
    "app"
  ],
  "author": "Arjun Komath <arjunkomath@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.8",
    "react": "^0.14.1",
    "react-dom": "^0.14.1"
  },
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "electron": "^0.4.1",
    "react-linkify": "^0.1.1",
    "react-loader": "^2.4.0",
    "react-photonkit": "^0.4.1",
    "react-slick": "^0.12.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
